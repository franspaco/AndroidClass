<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.franspaco.tareacomponentes.Activity3" android:orientation="vertical">

    <TextView
            android:text="@string/p8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView11"/>
    <ToggleButton
            android:textOn="Si"
            android:textOff="No"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/p8" android:layout_weight="1"/>
    <Space
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:layout_weight="1"/>
    <TextView
            android:text="@string/p9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView12"/>
    <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textMultiLine"
            android:ems="10"
            android:id="@+id/p9" android:layout_weight="5.67"/>
    <Space
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:layout_weight="1"/>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1">
        <Space
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_width="match_parent"/>
        <Button
                android:text="Siguiente"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:id="@+id/button" android:layout_weight="1"
                android:onClick="onClick_Next"/>
    </LinearLayout>
</LinearLayout>
